						"body": {
							"mode": "raw",
							"raw": "{\r\n  \"description\": \"Notify STH-Comet of all Heart Rate changes\", // Descrição da notificação\r\n  \"subject\": {\r\n    \"entities\": [\r\n      {\r\n        \"id\": \"urn:ngsi-ld:Munhequeira:001\",\r\n        \"type\": \"Munhequeira\"\r\n      }\r\n    ],\n    \"condition\": { \"attrs\": [\"batimento\"] } // Condição para acionar a notificação (alteração de batimento)\r\n  },\r\n  \"notification\": {\r\n    \"http\": {\r\n      \"url\": \"http://{{url}}:8666/notify\" // URL para notificação do STH-Comet\r\n    },\r\n    \"attrs\": [\r\n      \"batimento\" // Atributo a ser notificado (batimento cardíaco)\r\n    ],\r\n    \"attrsFormat\": \"legacy\" // Formato dos atributos a ser notificado (legado)\r\n  }\r\n}"
