								"type": "text"
							},
							{
								"key": "fiware-servicepath",
								"value": "/",
								"type": "text"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"devices\": [\n    {\n      \"device_id\": \"munhequeira001\",         // Identificador único do dispositivo\n      \"entity_name\": \"urn:ngsi-ld:Munhequeira:001\",   // Nome da entidade para o dispositivo (formato URN)\n      \"entity_type\": \"Munhequeira\",          // Tipo da entidade (neste caso, \"Munhequeira\")\n      \"protocol\": \"PDI-IoTA-UltraLight\",  // Protocolo usado para a comunicação (no exemplo, PDI-IoTA-UltraLight)\n      \"transport\": \"MQTT\",            // Método de transporte (neste caso, MQTT)\n\n      // Lista de comandos que o dispositivo pode receber\n      \"commands\": [\n        { \"name\": \"alertaOff\", \"type\": \"command\" }  // Comando para desligar o alerta (buzzer)\n      ],\n\n      // Lista de atributos associados ao dispositivo\n      \"attributes\": [\n        { \"object_id\": \"s\", \"name\": \"state\", \"type\": \"Text\" }, // Atributo de estado (Texto) - Ligado/Desligado\n        { \"object_id\": \"b\", \"name\": \"batimento\", \"type\": \"Integer\" },  // Atributo de batimento cardíaco (Inteiro)\n        { \"object_id\": \"t\", \"name\": \"temperatura\", \"type\": \"Float\" },  // Atributo de temperatura corporal (Float)\n        { \"object_id\": \"c\", \"name\": \"calorias\", \"type\": \"Float\" }  // Atributo de calorias gastas (Float)\n      ]\n    }\n  ]\n}\n"
						},
						"url": {
							"raw": "http://{{url}}:4041/iot/devices",
							"protocol": "http",
							"host": [
								"{{url}}"
							],
							"port": "4041",
							"path": [
